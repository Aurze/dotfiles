! Xft.dpi:		96
Xft.antialias: 	true
Xft.hinting: 	true
Xft.rgba: 		rgb
Xft.autohint: 	false
Xft.hintstyle: 	hintslight
Xft.lcdfilter: 	lcddefault

#define base_foreground #fffaf4
#define base_background #0e1019
#define base_color0 	#232323
#define base_color1 	#ff000f
#define base_color2 	#8ce10b
#define base_color3 	#ffb900
#define base_color4 	#008df8
#define base_color5 	#6d43a6
#define base_color6 	#00d8eb
#define base_color7 	#ffffff
#define base_color8 	#444444
#define base_color9 	#ff2740
#define base_color10 	#abe15b
#define base_color11 	#ffd242
#define base_color12 	#0092ff
#define base_color13 	#9a5feb
#define base_color14 	#67fff0
#define base_color15 	#ffffff

*foreground: base_foreground
*background: base_background
!black
*color0:  base_color0
*color8:  base_color8
!red
*color1:  base_color1
*color9:  base_color9
!green
*color2:  base_color2
*color10: base_color10
!yellow
*color3:  base_color3
*color11: base_color11
!blue
*color4:  base_color4
*color12: base_color12
!magenta
*color5:  base_color5
*color13: base_color13
!cyan
*color6:  base_color6
*color14: base_color14
!white
*color7:  base_color7
*color15: base_color15

#define base_foreground 		#ffffff
#define base_background 		#2f343f
#define base_dark_background 	#2a313b
#define base_light_background 	#414a59
#define base_primary 			#5294e2
#define base_warn       		#cc575d

*light_background: base_light_background
*dark_background: 	base_dark_background
*primary: 	  		base_primary
*warn: 	  		base_warn

! Rofi Settings
rofi.show-icons:      true
rofi.drun-icon-theme: Arc


!xTerm*FaceName: Noto
!xTerm*FaceSize: 9

! ! ------------------------------------------------------------------------------
! ! --- generated with 4bit Terminal Color Scheme Designer -----------------------
! ! ------------------------------------------------------------------------------
! ! --- http://ciembor.github.com/4bit -------------------------------------------
! ! ------------------------------------------------------------------------------
!
! ! --- special colors ---
!

! utils
URxvt.perl-lib: /usr/lib/urxvt/perl/
!URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
URxvt*colorUL: yellow
URxvt*colorIT: #87af5f
URxvt*colorBD: #d7d7d7
URxvt.perl-ext-common: selection-to-clipboard
! URxvt*saveLines: 0

! keymap
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~

! Appearence
URxvt*visualBell: false

!
! fonts
!
! xft fonts
URxvt*font: xft:DejaVu Sans Mono Nerd Font:bold:pixelsize=11, xft:Inconsolata\ for\ Powerline:pixelsize=11
!URxvt*boldFont: xft:DejaVu Sans Mono Nerd Font:bold:pixelsize=11
!URxvt*font: xft:DejaVu\ Sans\ Mono\ for\ Powerline:pixelsize=11,xft:Inconsolata\ for\ Powerline:pixelsize=11
!
! scrollbar
!
! enable scrollbar
URxvt*scrollBar: false     
! scrollbar width
URxvt*thickness: 5 
! scrollbar color
URxvt*scrollColor: #1793D1
! scrollbar without a trough
URxvt*scrollBar_floating: true
! scrollbar to the right
! URxvt*scrollBar_right:    true
! scrollbar style
URxvt*scrollstyle: plaine
